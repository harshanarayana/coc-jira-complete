{
  "name": "coc-jira-cplt",
  "version": "0.1.0",
  "author": "harshanarayana",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/harshanarayana/coc-jira-complete"
  },
  "main": "index.js",
  "keywords": [
    "jira",
    "autocomplete",
    "coc.nvim"
  ],
  "engines": {
    "coc": ">=0.0.57"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "j.url": {
          "type": "string",
          "default": "",
          "description": "The url of the JIRA workspace"
        },
        "j.user": {
          "type": "string",
          "default": "",
          "description": "JIRA user email"
        },
        "j.password": {
          "type": "string",
          "default": "",
          "description": "Base64 Encoded Password"
        }
      }
    }
  },
  "dependencies": {
    "coc.nvim": "^0.0.73",
    "jira-connector": "^3.1.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  }
}
